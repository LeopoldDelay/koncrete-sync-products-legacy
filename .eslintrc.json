{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "comma-dangle": ["error", "never"],
    "no-warning-comments": 0,
    "max-len":0,
    "implicit-arrow-linebreak": ["warn", "beside"],
    "function-paren-newline": ["warn"],
    "no-unused-vars": 0,
    "linebreak-style": 0,
    "import/no-unresolved":0,
    "consistent-return":0,
    "no-console":0,
    "new-cap":0,
    "no-underscore-dangle": 0
    // "no-restricted-syntax": ["ForInStatement"]
  }
}